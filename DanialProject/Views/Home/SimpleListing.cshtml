    <script type="text/javascript">
        var gValue = 1;
        function ChangeDisability(id) {
            if (id === "RESIDENTIAL") {
                $('#RESIDENTIAL').removeClass('disablilty');
                $('#COMMERCIAL').addClass('disablilty');
            }
            else {
                $('#COMMERCIAL').removeClass('disablilty');
                $('#RESIDENTIAL').addClass('disablilty');
            }
        }
        function filtersCheck() {
            if (gValue == 1) {
                $("input[name='Amenities']").each(function () {
                    $(this).prop('checked', true);
                });
                gValue = 0;
                document.getElementById("select-all").innerHTML = "Un-Select All";
            }
            else {
                $("input[name='Amenities']").each(function () {
                    $(this).prop('checked', false);
                });
                gValue = 1;
                document.getElementById("select-all").innerHTML = "Select All";
            }
        }
        $(document).ready(function (e) {
            SetFilterBox();
            RefreshPage();
            SetFilterFeaturesBox();

            $('.close-mobile-navbar').on('click', function (e) {
                $(this).closest('.top-navbar-mobile').removeClass('show-mobile-navbar');
            });
            $('.mobile-nav-opener').on('click', function (e) {
                $(this).closest('.navbar').find('.top-navbar-mobile').addClass('show-mobile-navbar');
            });

        });
        function Redirect(id) {
            window.location = '../../Home/SingleListing/' + id;
        }
        var name = "";
        var address = "";
        function RefreshPage() {
            $("#preloader").show();
            $("#listing-id").html('');
            $.ajax({
                type: "GET",
                url: "/api/WebApis/GetSimpleBuildings?filter=" + Filter + "", //URI
                dataType: "json",
                success: function (data) {
                    debugger;
                    var datavalue = data;
                    var myJsonObject = datavalue;
                    contentType: "application/json";
                    $.each(myJsonObject, function (i, mobj) {
                        var sp = mobj.Address.split(",");
                        var name = sp[0];
                        var address = "";
                        if (sp[0] == mobj.Name) {
                            address = "";
                        }
                        else {
                            address = mobj.Name;
                        }
                        var n1 = name.split(" ");
                        var address_for_users = "";
                        for (var y = 1; y < n1.length; y++) {
                            address_for_users = address_for_users + n1[y] + " ";
                        }

                        if (mobj.Type === "Sale") {
                            address_for_users = name;
                        }
                        var noFeeFilter;
                        if (mobj.Type === "Rent" && mobj.No_Fee == true) {
                            noFeeFilter = `<div class="card-img-box" style="height: 200px; position: relative; ">
                                <img src = "${mobj.Image}" class="img-responsive center" onclick = "Redirect(${mobj.ID});" style = "cursor:pointer; width:auto; height:100%;" />
                                <img src="../../Content/Template/assets/imgs/Icons/nofree.png" class="nofee-img" />
                            </div>`;
                        }
                        else {
                            noFeeFilter = ` <div class="card-img-box" style="height:200px;">
                                    <img src="${mobj.Image}" class="img-responsive center" onclick="Redirect(${mobj.ID});" style="cursor:pointer; width:auto; height:100%;" />
                                </div>`;

                        }
                        $("#listing-id").append(`<div class="col-md-4 pt-2 pl-2 pr-0">
                                        <div class="card" style="min-height:430px; ">
                                            ${noFeeFilter}
                                            <div class="card-content">
                                                <a href="../../Home/SingleListing/${mobj.ID}" class="user-link"><h1 class="font1 uppeer-case ">${address_for_users}</h1></a>
                                                <span class="room-short-text font1 uppeer-case ">${address}</span>
                                                <div class="row m-t-15">
                                                    <div class="col">
                                                        <small>Price</small><br />
                                                        <h5 class="rate font1">${dotDivider(mobj.Price)}</h5>
                                                    </div>
                                                    <div class="col">
                                                        <small>Rooms</small><br />
                                                        <h5 class="rate font1 uppeer-case ">${mobj.Beds} BED / ${mobj.Baths} BATH</h5>
                                                    </div>
                                                </div>
                                                <div class="row">
                                                    <div class="col">
                                                        <small>BOROUGH</small><br />
                                                        <h5 class="rate font1 uppeer-case">${mobj.Borough}</h5>
                                                    </div>
                                                    <div class="col">
                                                        <small>NEIGHBORHOOD</small><br />
                                                        <h5 class="rate font1 uppeer-case ">${mobj.Neighbourhood}</h5>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>`);
                    });
                    $("#preloader").hide();
                },
                error: function (xhr) {
                    bootbox.alert('Internel Error...');
                    $("#preloader").hide();
                }
            });
        }
        $('.filter-left-collapse').click(function () {
            $('#filter-card').css({
                left: '-700px'
            });
            $(this).hide();
            setTimeout(function () {
                $('a.filter-right-open').addClass('active');
            }, 500);

            $('#listings_wrapper').removeClass('col-sm-9');
            $('#listings_wrapper').removeClass('offset-sm-3');
            $('#listings_wrapper').addClass('col-sm-11');
            $('#listings_wrapper').addClass('offset-sm-1');


        });
        $('.filter-right-open').click(function () {
            $(this).removeClass('active');
            $('#filter-card').css({
                left: '40px'
            });
            setTimeout(function () {
                $('a.filter-left-collapse').show();
            }, 500);

            $('#listings_wrapper').removeClass('col-sm-11');
            $('#listings_wrapper').removeClass('offset-sm-1');
            $('#listings_wrapper').addClass('col-sm-9');
            $('#listings_wrapper').addClass('offset-sm-3');

        });
