    <div class="container-fluid m-t-10">
        <div class="row" style="margin-left:-4px; margin-right:-4px;">
            <div class="col-12 col-sm-4 p-0">
                <div class="side-bar-2" style="height:504px;position: relative;">
                    <div class="row">
                        <div class="col">
                            <div class="upper-content">
                                <h5 class="font1">
                                    <a href="../../Home/SimpleListing" class="float-left font1 link cursor-pointer all-listings"><i class="fas fa-arrow-left font-14"></i> &nbsp;&nbsp;All LISTINGS</a>
                                    <i class="far fa-heart float-right" style="font-size:25px;color:#838d96" onmouseover="this.style.color='red'" onmouseout="this.style.color='#838d96'"></i>
                                </h5>
                            </div>
                        </div>
                    </div>
                    <div class="all" style="position: absolute;bottom: 15px;left: 25px;">
                        <div class="row">
                            <div class="col">
                                @{
                                    var price = "";
                                    if (Model.Price.ToString().Contains("."))
                                    {

                                        var splitdot = Model.Price.ToString().Split('.');
                                        var toIntValue = Int32.Parse(splitdot[1]);

                                        if (toIntValue > 0)
                                        {
                                            price = @String.Format("{0:C2}", Convert.ToDouble(Model.Price));
                                        }
                                        else
                                        {
                                            price = @String.Format("{0:C0}", Convert.ToDouble(Model.Price));
                                        }

                                    }
                                    else
                                    {
                                        price = @String.Format("{0:C0}", Convert.ToDouble(Model.Price));
                                    }

                                    var sp1 = Model.Address.Split(',');
                                    var name1 = sp1[0];
                                    var address1 = "";
                                    if (sp1[0] == Model.Name)
                                    {
                                        address1 = "";
                                    }
                                    else
                                    {
                                        address1 = Model.Name;
                                    }
                                    if (Model.Type == "Rent")
                                    {
                                        if (Model.OtherDetails.Exclusivity != "Exclusive")
                                        {
                                            var n1 = name1.Split(' ');
                                            var address_for_users = "";
                                            for (var y = 1; y < n1.Length; y++)
                                            {
                                                address_for_users = address_for_users + n1[y] + " ";
                                            }
                                            <div class="lower-content" style="margin-bottom:25px !important;">
                                                <h2 class="uppeer-case" style="margin-bottom: 1px;font-size: 50px;">@address_for_users @Model.Borough</h2>
                                                <h4 class="uppeer-case" style="font-size: 31px;">@address1</h4>
                                                <small class="m-t-15 uppeer-case"><STRONG>UNIT:</STRONG> @Model.Unit</small>
                                            </div>
                                        }
                                        else
                                        {
                                            <div class="lower-content" style="margin-bottom:25px !important;">
                                                <h2 class="uppeer-case" style="margin-bottom: 1px;font-size: 50px;">@name1 @Model.Borough</h2>
                                                <h4 class="uppeer-case" style="font-size: 31px;">@address1</h4>
                                                <small class="m-t-15 uppeer-case"><STRONG>UNIT:</STRONG> @Model.Unit</small>
                                            </div>
                                        }
                                    }
                                    else
                                    {
                                        <div class="lower-content" style="margin-bottom:25px !important;">
                                            <h2 class="uppeer-case" style="margin-bottom: 1px;font-size: 50px;">@name1 @Model.Borough</h2>
                                            <h4 class="uppeer-case" style="font-size: 31px;">@address1</h4>
                                            <small class="m-t-15 uppeer-case"><STRONG>UNIT:</STRONG> @Model.Unit</small>
                                        </div>
                                    }
                                }
                            </div>
                        </div>
                        <div class="row m-t-25 single_listing_rates_wrapper">
                            <div class="col-md-6 rates">
                                <small>Price</small><br />
                                <h5 class="rate font1">@price</h5>
                            </div>
                            <div class="col-md-6 rates">
                                <small>Rooms</small><br />
                                @{
                                    try
                                    {
                                        var Bed_Text = "BEDS";
                                        var Baths_Text = "BATHS";
                                        var bed = Model.Beds;
                                        var bath = Model.Baths;
                                        if (bed <= 1 && bath <= 1)
                                        {
                                            Bed_Text = "BED";
                                            Baths_Text = "BATH";
                                        }
                                        else if (bed > 1 && bath <= 1)
                                        {
                                            Bed_Text = "BEDS";
                                            Baths_Text = "BATH";
                                        }
                                        else if (bed <= 1 && bath > 1)
                                        {
                                            Bed_Text = "BED";
                                            Baths_Text = "BATHS";
                                        }
                                        <h5 class="rate font1">@Model.Beds @Bed_Text / @Model.Baths @Baths_Text</h5>
                                    }
                                    catch (Exception)
                                    {
                                        <h5 class="rate font1">@Model.Beds BEDS / @Model.Baths BATHS</h5>
                                    }
                                }
                            </div>
                            <div class="col-md-6 rates">
                                <div class="op">
                                    <small>BOROUGH</small><br />
                                    <h5 class="rate font1 uppeer-case ">@Model.Borough</h5>
                                </div>
                            </div>
                            <div class="col-md-6 rates">
                                <div class="op">
                                    <small>NEIGHBORHOOD</small><br />
                                    <h5 class="rate font1 uppeer-case">@Model.Neighbourhood</h5>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
